	</item>	<item datasource="sections" inline="true">	<key>93c22144-427d-d0e1-c920-ffcdb006e591</key>
	<value>Giới thiệu</value>
<component id="93c22144-427d-d0e1-c920-ffcdb006e591">
	<content-type>/component/homeitem1</content-type>
	<display-template>/templates/web/homeitem1.ftl</display-template>
